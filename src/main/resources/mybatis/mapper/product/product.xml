<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.knpharm.minol.dao.ProductDAO">

    <!-- 약국 리스트 조회 -->
    <select id="listProduct" parameterType="com.knpharm.minol.domain.ProductVO" resultType="com.knpharm.minol.domain.ProductVO">
        <![CDATA[
        SELECT
            *
        FROM TBL_PRODUCT
        WHERE BRAND_CODE = #{brandCode}
        ORDER BY PRODUCT_ORDER ASC
        ]]>
    </select>

</mapper>